<%= simple_form_for(@contacto) do |f| %>
  <%= f.error_notification %>
  <div class="container">
    <div class="form-inputs">
      <%= f.input :Nombre %>
      <%= f.input :Email %>
      <%= f.input :Asunto %>
      <%= f.input :Body %>
    </div>

    <div class="form-actions">
      <%= f.submit  "Enviar formulario"%>
    </div>
  </div>
<% end %>
